<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="{{ grav.language.getLanguage ?: 'en' }}"> <![endif]-->
<!--[if (IE 7)&!(IEMobile)]><html class="no-js lt-ie9 lt-ie8" lang="{{ grav.language.getLanguage ?: 'en' }}"><![endif]-->
<!--[if (IE 8)&!(IEMobile)]><html class="no-js lt-ie9" lang="{{ grav.language.getLanguage ?: 'en' }}"><![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="{{ grav.language.getLanguage ?: 'en' }}"><!--<![endif]-->
<head>
{% include 'partials/head.html.twig' %}
</head>
<body>

{% if config.plugins.login.enabled and grav.user.authorize('site.login') %}
{% set uservalid = true %}

{% include 'threejs/threejs-utils.html.twig' with { 'sceneid': sceneid, 'projectid': projectid } %}
{% include 'threejs/threejs-tools.html.twig' with { 'sceneid': sceneid, 'projectid': projectid } %}

{% include 'menus/navigation-sim.html.twig' with { 'sceneid': sceneid, 'projectid': projectid } %}
{% endif %}

{% block header %}
    <div class="entry-header">
      <div class="header-title">
        <h2>{{ page.title }}</h2>
      </div><!-- /.header-title -->
    </div><!-- /.entry-header -->
{% endblock %}

<div id="main" role="main">
 {% block content %}{% endblock %}
</div><!-- /#main -->

<div class="footer-wrapper">
  <footer role="contentinfo">
    {% include 'partials/footer.html.twig' %}
  </footer>
</div><!-- /.footer-wrapper -->

{% include 'partials/scripts.html.twig' %}

</body>
</html>
