<script>
$(document).ready(function() {
  {% set query = "[sql-table json]select * from projects where users like '%{{ grav.user.username }}%'[/sql-table]" %}
  {% set results = (query|shortcodes)| json_decode %}

  {% for pp in results %}  
  {% if pp %}

    {% set query = ("[sql-table json]select * from scenes where projectid = '" ~ pp.uid ) ~ "' [/sql-table]" %}
    {% set scenedb = ((query|shortcodes) | json_decode) %}
    {% if scenedb %}
    {% set randomscene = random(scenedb) %}

    {% set iconquery = ("[sql-table json]select * from icons where uid = '" ~ randomscene.icon ) ~ "' [/sql-table]" %}
    {% set icondb = ((iconquery|shortcodes) | json_decode)[0] %}

    $("#{{ pp.uid|md5 }}").html('<img src="data:image/png;base64,{{ icondb.data }}" />');

    $("#{{ pp.uid|md5 }}").on('click', function(e) {
      const params = jQuery.param({ 
          'projectid': '{{ pp.uid }}'
      });
      window.location.href = "/project?" + params;       
    });

    {% endif %}
  {% endif %}
  {% endfor %}});
</script>

