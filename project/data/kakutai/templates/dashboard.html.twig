{% set query = "[sql-table json]select name,uid,desc from projects where users like '%{{ grav.user.username }}%' ORDER BY modified[/sql-table]" %}
{% set results = (query|shortcodes)| json_decode %}

{% set iiquery = ('[sql-table json]select * from issues') ~ ' [/sql-table]' %}
{% set itable = ( iiquery|shortcodes ) | json_decode %}


